  "settings": {
    "index": {
      "routing": {
        "allocation": {
          "include": {
            "_tier_preference": "data_content"
          }
        }
      },
      "mapping": {
        "total_fields": {
          "limit": "5000"
        },
        "ignore_malformed": "true"
      },
      "number_of_shards": "5",
      "max_shingle_diff": "8",
      "max_result_window": "1000000",
      "analysis": {
        "filter": {
          "italian_stop_filter": {
            "type": "stop",
            "stopwords": "_italian_"
          },
          "shingle_filter": {
            "max_shingle_size": "5",
            "min_shingle_size": "2",
            "type": "shingle"
          },
          "ep_synonyms_filter": {
            "type": "synonym_graph",
            "lenient": "true",
            "synonyms": [
              "sneakers, tennis shoes, trainers, runners",
              "buddha, budda, Buddha, Budda",
              "shoes => sneaker, sandal, boots, high heels",
              "dèi => dei, dèi",
              "buddha => budda"
            ]
          },
          "edge_ngram": {
            "min_gram": "3",
            "side": "front",
            "type": "edge_ngram",
            "max_gram": "10"
          },
          "ewp_word_delimiter": {
            "type": "word_delimiter_graph",
            "preserve_original": "true"
          },
          "ewp_snowball": {
            "type": "snowball",
            "language": "Italian"
          }
        },
        "normalizer": {
          "lowerasciinormalizer": {
            "filter": ["lowercase", "asciifolding"],
            "type": "custom"
          }
        },
        "analyzer": {
          "ewp_lowercase": {
            "filter": ["lowercase"],
            "type": "custom",
            "tokenizer": "keyword"
          },
          "with_stopwords": {
            "filter": ["ep_synonyms_filter", "lowercase"],
            "char_filter": ["html_strip"],
            "language": "italian",
            "tokenizer": "standard"
          },
          "default": {
            "filter": [
              "ep_synonyms_filter",
              "ewp_word_delimiter",
              "lowercase",
              "italian_stop_filter",
              "ewp_snowball"
            ],
            "stopwords": "_italian_",
            "char_filter": ["html_strip"],
            "language": "italian",
            "tokenizer": "standard"
          },
          "edge_ngram_analyzer": {
            "filter": ["lowercase", "edge_ngram"],
            "type": "custom",
            "tokenizer": "standard"
          },
          "shingle_analyzer": {
            "filter": ["lowercase", "shingle_filter"],
            "type": "custom",
            "tokenizer": "standard"
          }
        }
      }
    }
  }
